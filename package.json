{
  "name": "react-power-select",
  "version": "1.0.0-beta.18",
  "description": "A highly composable & reusable select component in react",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf lib",
    "build:commonjs": "babel src/ --out-dir lib/",
    "build": "npm run clean && npm run build:commonjs && webpack && rm -rf dist/css.js",
    "prepublish": "npm run build",
    "precommit": "lint-staged",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "jest": {
    "automock": false,
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text",
      "lcov"
    ],
    "roots": [
      "src"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/jestEnvironment.js",
    "setupFiles": [
      "./testSetup.js"
    ],
    "unmockedModulePathPatterns": [
      "node_modules",
      "babel"
    ],
    "testPathIgnorePatterns": [
      "src/__tests__/test-utils"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --print-width 100 --single-quote --trailing-comma es5 --write",
      "git add"
    ],
    "stories/**/*.js": [
      "prettier --print-width 100 --single-quote --trailing-comma es5 --write",
      "git add"
    ],
    "docs/app/Demos/**/*.js": [
      "prettier --print-width 100 --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selvagsz/react-power-select.git"
  },
  "author": "selvagsz (selvaganeshbsg@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/selvagsz/react-power-select/issues"
  },
  "keywords": [
    "react-power-select",
    "react-autocomplete",
    "react-select",
    "react-typeahead",
    "react-multiselect",
    "react-component"
  ],
  "files": [
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/selvagsz/react-power-select#readme",
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addon-links": "^3.4.0",
    "@storybook/addon-options": "^3.4.0",
    "@storybook/addons": "^3.4.0",
    "@storybook/react": "^3.4.0",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "codemirror": "^5.27.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.4",
    "jest": "^20.0.4",
    "jquery": "^3.3.1",
    "jsdom": "^11.6.2",
    "lint-staged": "^3.6.1",
    "minimist": "^1.2.0",
    "multi-loader": "^0.1.0",
    "node-sass": "7.0.1",
    "prettier": "^1.6.1",
    "raf": "^3.4.0",
    "react": "16.7.0-alpha.2",
    "react-codemirror2": "^4.0.0",
    "react-dom": "16.7.0-alpha.2",
    "react-test-renderer": "16.7.0-alpha.2",
    "regenerator-runtime": "^0.10.5",
    "sass-loader": "^6.0.5",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.10",
    "react-tether": "^0.6.1"
  }
}
